# YagoutPay SDK Configuration
# Copy this file to .env and fill in your actual values

# Your YagoutPay merchant credentials
MERCHANT_ID=your_merchant_id
ENCRYPTION_KEY=your_base64_32byte_key

# YagoutPay gateway URLs
YAGOUT_UAT_URL=https://uatcheckout.yagoutpay.com/ms-transaction-core-1-0/paymentRedirection/checksumGatewayPage
YAGOUT_PROD_URL=https://checkout.yagoutpay.com/ms-transaction-core-1-0/paymentRedirection/checksumGatewayPage

# Your application base URL (for callbacks)
BASE_URL=http://localhost:3000

# Demo application settings
DEMO_PORT=3000
NODE_ENV=development
